|| extract the codes from the output of the vm evaluation of challenge.bin

%import <base>
%import <io>                    (>>=)/io_bind

main :: io ()
main = getContents >>= words .> map (filter letter) .> filter isCode .> nub cmpstring .> lay .> putStrLn
       where
         isCode xs = any isUpper xs & #xs == 12
